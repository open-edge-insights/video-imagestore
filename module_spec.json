{
    "SchemaVersion": "0.2",
    "Runtime": "docker",
    "RuntimeOptions": {
        "ContainerImage": "ia_imagestore:$EIS_VERSION",
        "TemporaryVolumes": [
            "$EIS_INSTALL_PATH/data:/data"
        ]
    },
    "DataStore": {
        "Protocol": "DIRECT",
        "DataBuckets": [
            {
                "Name": "EIS/ImageStore",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/GlobalEnv",
                "Permissions": "RO"
            },
            {
                "Name": "EIS/Publickeys",
                "Permissions": "RO"
            }
        ]
    }
}
