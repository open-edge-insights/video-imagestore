{
    "Module" : {
        "Name": "ImageStore",
        "Description": "ImageStore module manifest",
        "SchemaVersion": "0.2",
        "ManifestFile": "${idx:imagestore:$EIS_VERSION}",
        "RunAsUser": "$EIS_UID",
        "RunAsGroup": "$EIS_UID",
        "Resources": {
            "CPU": 100,
            "MemoryMB": 100
        },
        "ExecutionEnv": {
            "AppName": "ImageStore",
            "ETCD_ENDPOINT": "${datastore.endpoint}",
            "CONFIGMGR_CACERT": "${databucket.cacert}",
            "CONFIGMGR_CERT": "${databucket.cert}",
            "CONFIGMGR_KEY": "${databucket.key}",
            "ETCD_PREFIX": "$ETCD_PREFIX",
            "DEV_MODE": "$DEV_MODE",
            "PROFILING_MODE": "false",
            "SERVER_ENDPOINT": "0.0.0.0:${ep.imsServer.localport}",
            "SUBSCRIBER_ENDPOINT": "${ep.imsSub.remoteaddress}:${ep.imsSub.remoteport}"
        },
        "Endpoints": [
            {
                "Name": "imsServer",
                "Endtype": "server",
                "Port": "5669",
                "DataType": "messages",
                "Link": "server-is"
            },
            {
                "Name": "imsSub",
                "Endtype": "client",
                "DataType": "messages",
                "Link": "va-output"
            }
        ]
    }
}
